{
	"builds": [
		{
			"src": "TrainStationMonitoringSystem/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"buildCommand": "./vercel-build.sh",
				"maxLambdaSize": "15mb",
				"runtime": "python3.12"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "TrainStationMonitoringSystem/wsgi.py"
		}
	]
}
